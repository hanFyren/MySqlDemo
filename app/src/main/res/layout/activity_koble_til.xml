<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.baard.mysqldemo.KobleTil">

    <Button
        android:id="@+id/buttonKobletil"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:text="Koble til din E4"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.392"
        app:layout_constraintHorizontal_bias="1.0"
        android:onClick="OnOppkobling"
        />

    <Button
        android:id="@+id/buttonStartLogging"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:text="Start Logging"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.008"
        app:layout_constraintHorizontal_bias="0.0"
        android:onClick="OnLogge"/>

    <Spinner
        android:id="@+id/spinner"
        android:layout_width="585dp"
        android:layout_height="wrap_content"
        android:autofillHints="Velg Enhet"
        android:background="@android:drawable/btn_dropdown"
        android:spinnerMode="dropdown"
        android:text="Velg enhet:"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/buttonOvervak"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:text="OvervÃ¥k logging"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.155"
        android:enabled="false"
        android:onClick="OnOvervak"/>


</android.support.constraint.ConstraintLayout>
